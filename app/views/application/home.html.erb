<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <header>
    <% if session[:user_id] %>
      <%= link_to "Profile", user_path(User.find session[:user_id]) %>
      <%= form_tag '/logout', method:"delete" do %>
        <%= submit_tag "Logout" %>
      <% end %>
    <% else %>
      <%= form_tag '/login', method:"get" do %>
        <%= submit_tag "Login" %>
      <% end %>
    <% end %>

    <%= form_tag '/search', method: "get" do %>
      <%= label_tag :q,"Search for phrases:" %>
      <%= text_field_tag :q %>
      <%= submit_tag "Search!" %>
    <% end %>

  </header>
  <body>
    <div class="">
      <form class="" action="/categorize" method="post">

      </form>
      <p>Categorize Terms by:</p>
        <li><%= link_to "Region", %></li>

    </div>
  </body>
</html>
